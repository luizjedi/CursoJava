import java.util.Locale;

public class Main {

	public static void main(String[] args) {
	   
	   String nome = "Maria";
	   int idade = 31;
	   double renda = 4000.0;
	
	   Locale.setDefault(Locale.US);
	   System.out.printf("%s tem %d anos e ganha R$ %.2f reais%n", nome, idade, renda);
	   
	}

}
// Teste Processamento

public class Main {

	public static void main(String[] args) {
		int x, y;
		
		x = 5;
		
		y = 2 * x;
		
		System.out.println(x);
		System.out.println(y);
		
	}